{% extends 'BlogBundle::layout.html.twig' %}

{% block body %}
  {% form_theme form 'BlogBundle::fields.html.twig' %}
  {{ form_start(form, {'attr': {'class': 'col s12'}}) }}

    {{ form_row(form.category) }}
    {{ form_row(form.title) }}
    {{ form_row(form.content) }}
